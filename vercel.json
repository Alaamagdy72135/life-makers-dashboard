{
  "version": 2,
  "builds": [
    {
      "src": "src/frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/src/frontend/build/$1"
    }
  ],
  "env": {
    "REACT_APP_API_URL": "https://life-makers-dashboard-backend.onrender.com"
  }
} 